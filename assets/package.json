{
  "repository": {},
  "license": "MIT",
  "scripts": {
    "deploy": "webpack --mode production",
    "test": "jest",
    "watch": "webpack --mode development --watch",
    "lint": "eslint -c .eslintrc.js --ext ts,tsx,js ./src ./tests --fix",
    "lint:check": "eslint -c .eslintrc.js --ext ts,tsx,js ./src ./tests",
    "format": "prettier --write \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "check": "tsc --noEmit && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^5.3.2",
    "@fullcalendar/react": "^5.3.1",
    "bootstrap": "^4.5.2",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "query-string": "^6.13.4",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-datepicker": "^3.2.2",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "rrule": "^2.6.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.24.3",
    "@testing-library/react": "^11.0.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.49",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.21",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^10.0.1",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.2",
    "prettier": "^2.1.2",
    "react-select-event": "^5.1.0",
    "sass-loader": "^9.0.3",
    "source-map-loader": "^1.1.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^8.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!@fullcalendar).+"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testRegex": "(src|tests)/.*\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [],
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.test.json"
      }
    }
  }
}
