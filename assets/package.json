{
  "repository": {},
  "license": "MIT",
  "scripts": {
    "deploy": "webpack --mode production",
    "test": "jest",
    "watch": "webpack --mode development --watch",
    "lint": "eslint -c .eslintrc.js --ext ts,tsx,js ./src ./tests --fix",
    "lint:check": "eslint -c .eslintrc.js --ext ts,tsx,js ./src ./tests",
    "format": "prettier --write \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "check": "tsc --noEmit && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@fullcalendar/daygrid": "^5.1.0",
    "@fullcalendar/react": "^5.1.0",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "react": "^16.13.1",
    "react-bootstrap": "^1.1.1",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "rrule": "^2.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-typescript": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/react": "^10.4.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.41",
    "@types/react-datepicker": "^3.0.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.6.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "source-map-loader": "^1.0.1",
    "terser-webpack-plugin": "^3.0.6",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!@fullcalendar).+"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(src|tests)/.*\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [],
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.test.json"
      }
    }
  }
}
