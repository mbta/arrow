{
  "name": "arrow",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "deploy": "webpack --mode production",
    "test": "jest",
    "watch": "webpack --mode development --watch",
    "lint": "eslint -c .eslintrc.js --ext ts,tsx,js ./src ./tests --fix",
    "lint:check": "eslint -c .eslintrc.js --ext ts,tsx,js ./src ./tests",
    "format": "prettier --write \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "check": "tsc --noEmit && npm run lint:check && npm run format:check",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^5.4.0",
    "@fullcalendar/react": "^5.4.0",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "query-string": "^6.13.7",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.1",
    "rrule": "^2.6.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.28.1",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.16",
    "@types/react": "^17.0.0",
    "@types/react-datepicker": "^3.1.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-select": "^3.0.27",
    "@types/react-test-renderer": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.9.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^10.0.4",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "npm-force-resolutions": "0.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.3",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "react-select-event": "^5.1.0",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^1.1.2",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^25.5.1",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "resolutions": {
    "@types/react": "^17.0.0"
  },
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!@fullcalendar).+"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testRegex": "(src|tests)/.*\\.test\\.tsx?$",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [],
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.test.json"
      }
    }
  }
}
